{
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false,
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
  },
  "globals": {
    "Phaser": true,
    "__DEV__": true,
    "__API_URL__": true,
    "__API_GAME_ID__": true,
  },
  "rules": {
    "comma-dangle": [2,"always-multiline"],
    "no-cond-assign": 2,
    "no-console": 0,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 2,
    "no-empty": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": [2,"both"],
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "valid-typeof": 2,
    "block-scoped-var": 2,
    "curly": [2,"all"],
    "default-case": 2,
    "dot-notation": 2,
    "eqeqeq": 2,
    "semi": [2, "always"],
    "quotes": [2, "single", { "allowTemplateLiterals": true }],
    "max-len": [1, 100],
    "no-alert": 2,
    "no-caller": 2,
    "no-else-return": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 1,
    "no-implicit-coercion": 1,
    "no-implied-eval": 1,
    "no-invalid-this": 2,
    "no-loop-func": 2,
    "no-multi-spaces": 2,
    "no-native-reassign": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-useless-call": 2,
    "no-delete-var": 2,
    "no-undef": 1,
    "no-unused-vars": 2,
    "no-use-before-define": 2,
    "brace-style": 1,
    "camelcase": 1,
    "comma-spacing": [1,{"after":true}],
    "comma-style": 1,
    "eol-last": 1,
    "indent": [2, 2, {"SwitchCase": 1}],
    "linebreak-style": [2,"unix"],
    "no-array-constructor": 1,
    "no-continue": 1,
    "no-lonely-if": 1,
    "no-mixed-spaces-and-tabs": 2,
    "no-trailing-spaces": 1,
    "arrow-parens": 1,
    "arrow-spacing": [1,{"before":true,"after":true}],
    "constructor-super": 1,
    "no-const-assign": 1,
    "no-this-before-super": 1,
    "no-var": 2,
    "prefer-const": 0,
    "prefer-spread": 1,
    "object-curly-spacing": [2, "always"],
    "array-bracket-spacing": [2, "never"],
    "computed-property-spacing": [2, "never"],
    "keyword-spacing": [2, {
      "after": true,
      "before": true
    }]
  }
}
